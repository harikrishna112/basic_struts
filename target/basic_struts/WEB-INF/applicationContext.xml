<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">  
  
<bean id="login" class="mypack.Login" name="login">  
<property name="message" value="Welcome Spring"></property>  
</bean>  
<bean id="Register" class="mypack.SignUpMember" name="Register">
<property name="message" value="User Registered Successfully."></property>  
</bean>
  
</beans>  